        <!--Patch options-->
        <phrase id="Patch_Name_SteamOneClickPatch">
            <translation locale="en_US">Steam One-Click</translation>
        </phrase>
        <phrase id="Patch_Description_SteamOneClickPatch">
            <translation locale="en_US">Attempts to set up the launcher for Steam for the Steam Deck by adding the launcher to Steam, prompting to change the controller layout, and disabling fullscreen. NOTE: For Linux users (specifically Flatpak users), after applying, if there is no prompt for the controller layout, it will be prompted later after a manual start of Steam.</translation>
        </phrase>
        <phrase id="Patch_Name_ModLoaderPatch">
            <translation locale="en_US">Mod Loader</translation>
        </phrase>
        <phrase id="Patch_Description_ModLoaderPatch">
            <translation locale="en_US">Allows the installation of client mods.</translation>
        </phrase>
        <phrase id="Patch_Name_AutoTcpUdpPatch">
            <translation locale="en_US">Auto TCP/UDP Shim</translation>
        </phrase>
        <phrase id="Patch_Description_AutoTcpUdpPatch">
            <translation locale="en_US">Enables connecting to community-run LEGO Universe servers that may or may not use TCP/UDP. This is automatically managed for the requested server. Requires the Mod Loader to be installed. Can't be installed with TCP/UDP Shim.</translation>
        </phrase>
        <phrase id="Patch_Name_TcpUdpPatch">
            <translation locale="en_US">TCP/UDP Shim</translation>
        </phrase>
        <phrase id="Patch_Description_TcpUdpPatch">
            <translation locale="en_US">Enables connecting to community-run LEGO Universe servers that use TCP/UDP, like Uchu. This mod prevents connecting to normal RakNet servers, like Darkflame Universe. Requires the Mod Loader to be installed. Can't be installed with Auto TCP/UDP Shim.</translation>
        </phrase>
        <phrase id="Patch_Name_FixAssemblyVendorHologramPatch">
            <translation locale="en_US">Fix Assembly Vendor Hologram</translation>
        </phrase>
        <phrase id="Patch_Description_FixAssemblyVendorHologramPatch">
            <translation locale="en_US">Fixes the Assembly vendor at Nimbus Station showing a Missing NIF error.</translation>
        </phrase>
        <phrase id="Patch_Name_FixAvantGardensSurvivalCrashPatch">
            <translation locale="en_US">Fix Avant Gardens Survival Crash</translation>
        </phrase>
        <phrase id="Patch_Description_FixAvantGardensSurvivalCrashPatch">
            <translation locale="en_US">Fixes a mistake in the Avant Gardens Survival script that results in players crashing in Avant Gardens Survival if they are not the first player.</translation>
        </phrase>
        <phrase id="Patch_Name_RemoveDluPatchAd">
            <translation locale="en_US">Remove DLU Ad</translation>
        </phrase>
        <phrase id="Patch_Description_RemoveDluPatchAd">
            <translation locale="en_US">Removes the advertisement for DLU from the zone loading screen. This is to return the localization file back to the original version.</translation>
        </phrase>
        
